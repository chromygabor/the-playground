<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane hgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.CounterController">
    <children>
        <Button fx:id="_btnDecrement" mnemonicParsing="false" text="Decrement" onAction="#decrementClicked" />
        <Label fx:id="_lblCounter" />
        <Button fx:id="_btnIncrement" mnemonicParsing="false" text="Increment" onAction="#incrementClicked" />
        <Button fx:id="_btnClose" mnemonicParsing="false" text="X" onAction="#closeClicked" />
    </children>
</FlowPane>
